---
import PageLayout from "@/layouts/Base.astro";
import Section from "@/components/Section.astro";
import ContactCard from "@/components/ContactCard.astro";

const meta = {
  description:
    "Program - UR-RAD 2025 AAAI Fall Symposium on Unifying Representations for Robot Application Development",
  title: "Program",
};

const headingMonthMap: Record<string, string> = {
  january: "01",
  february: "02",
  march: "03",
  april: "04",
  may: "05",
  june: "06",
  july: "07",
  august: "08",
  september: "09",
  october: "10",
  november: "11",
  december: "12",
};

function headingToIsoDate(heading: string, year: number = 2025): string | null {
  const m = heading.match(
    /\b(January|February|March|April|May|June|July|August|September|October|November|December)\s+(\d{1,2})(?:st|nd|rd|th)?\b/i,
  );
  if (!m) return null;
  const monthPart = m[1];
  const dayPart = m[2];
  if (!monthPart || !dayPart) return null;
  const monthName = monthPart.toLowerCase();
  const dayNum = Number.parseInt(dayPart, 10);
  if (!Number.isFinite(dayNum)) return null;
  const mm = headingMonthMap[monthName];
  if (!mm) return null;
  const dd = String(dayNum).padStart(2, "0");
  return `${year}-${mm}-${dd}`;
}

